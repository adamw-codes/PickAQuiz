<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".quizNameSetFragment">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:id="@+id/quizSetFormat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Quiz File Name: "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/quizFileSet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="default"
        app:layout_constraintStart_toEndOf="@+id/quizSetFormat"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/quizSetName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Quiz Name: "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/quizSetFormat" />

    <TextView
        android:id="@+id/quizNameSet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="default"
        app:layout_constraintStart_toEndOf="@+id/quizSetName"
        app:layout_constraintTop_toBottomOf="@+id/quizSetFormat" />

</androidx.constraintlayout.widget.ConstraintLayout>